#: modules/migrator/info.php:3
msgid "Migration Assistant"
msgstr "Assistente per la migrazione"

#: modules/migrator/info.php:6
msgid "Enables import from Wordpress, MovableType, TextPattern, and Tumblr."
msgstr ""
"Abilita l'importazione da Wordpress, MovableType, TextPattern e Tumblr."

#: modules/migrator/migrator.php:16
msgid "Migration"
msgstr "Migrazione"

#: modules/migrator/migrator.php:33 modules/migrator/migrator.php:254
#: modules/migrator/migrator.php:455 modules/migrator/migrator.php:621
msgid "You do not have sufficient privileges to import content."
msgstr "Non si dispone di privilegi sufficienti per importare contenuti."

#: modules/migrator/migrator.php:47 modules/migrator/migrator.php:273
#: modules/migrator/migrator.php:469 modules/migrator/migrator.php:635
msgid "Missing Feather"
msgstr "Feather mancante"

#: modules/migrator/migrator.php:48
msgid "Text feather must be enabled to import from WordPress."
msgstr ""
"La Feather di Testo deve essere abilitata per l'importazione da WordPress."

#: modules/migrator/migrator.php:55
msgid "You must select a WordPress export file."
msgstr "È necessario selezionare un file di esportazione WordPress."

#: modules/migrator/migrator.php:131
msgid "The file does not seem to be a valid WordPress export file."
msgstr "Il file non sembra essere un file di esportazione WordPress valido."

#: modules/migrator/migrator.php:238
msgid "WordPress content successfully imported!"
msgstr "Contenuti WordPress importati con successo!"

#: modules/migrator/migrator.php:274
msgid ""
"Text, Photo, Quote, and Link feathers must be enabled to import from Tumblr."
msgstr ""
"La Feather di Testo, Foto, Citazione e Link devono essere abilitate per "
"l'importazione da Tumblr."

#: modules/migrator/migrator.php:281
msgid "Invalid URL."
msgstr "URL non valido."

#: modules/migrator/migrator.php:308
msgid "Could not retrieve content from the Tumblr URL."
msgstr "Impossibile recuperare il contenuto dall'URL di Tumblr."

#: modules/migrator/migrator.php:439
msgid "Tumblr content successfully imported!"
msgstr "Contenuti Tumblr importati con successo!"

#: modules/migrator/migrator.php:470
msgid "Text feather must be enabled to import from TextPattern."
msgstr ""
"La Feather di Testo deve essere abilitata per l'importazione da TextPattern."

#: modules/migrator/migrator.php:477 modules/migrator/migrator.php:643
msgid "MySQLi is required for database migration."
msgstr "MySQLi è necessario per la migrazione del database."

#: modules/migrator/migrator.php:484 modules/migrator/migrator.php:650
msgid "Host cannot be empty."
msgstr "L'Host non può essere vuoto."

#: modules/migrator/migrator.php:491 modules/migrator/migrator.php:657
msgid "Username cannot be empty."
msgstr "Il nome utente non può essere vuoto."

#: modules/migrator/migrator.php:498 modules/migrator/migrator.php:664
msgid "Password cannot be empty."
msgstr "Inserire la password."

#: modules/migrator/migrator.php:505 modules/migrator/migrator.php:671
msgid "Database cannot be empty."
msgstr "Il database non può essere vuoto."

#: modules/migrator/migrator.php:524
msgid "Could not connect to the TextPattern database."
msgstr "Impossibile connettersi al database TextPattern."

#: modules/migrator/migrator.php:539 modules/migrator/migrator.php:702
msgid "Database Error"
msgstr "Errore di database"

#: modules/migrator/migrator.php:605
msgid "TextPattern content successfully imported!"
msgstr "Contenuto TextPattern importato con successo!"

#: modules/migrator/migrator.php:636
msgid "Text feather must be enabled to import from MovableType."
msgstr ""
"La Feather di Testo deve essere abilitata per l'importazione da MovableType."

#: modules/migrator/migrator.php:690
msgid "Could not connect to the MovableType database."
msgstr "Impossibile connettersi al database MovableType."

#: modules/migrator/migrator.php:796
msgid "MovableType content successfully imported!"
msgstr "Il contenuto di MovableType è stato importato con successo!"
